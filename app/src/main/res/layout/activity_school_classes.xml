<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nestedmango.digitalschool.SchoolClassesActivity">

    <TextView
        android:id="@+id/textViewclass"
        android:layout_width="106dp"
        android:layout_height="51dp"
        android:text="Classes"
        android:textColor="@color/colorPrimary"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="16dp" />

    <CheckBox
        android:id="@+id/checkBoxplayschool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Play School"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="98dp" />

    <CheckBox
        android:id="@+id/checkBoxnur5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nur -5th "
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="130dp" />

    <CheckBox
        android:id="@+id/checkBoxnur8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nur - 8th"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="162dp" />

    <CheckBox
        android:id="@+id/checkBoxnur10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nur - 10th"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="194dp" />

    <CheckBox
        android:id="@+id/checkBoxnur12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nur - 12th"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="224dp" />

    <Button
        android:id="@+id/buttonnext1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="280dp" />

    <Button
        android:id="@+id/buttonskip1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Skip"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="338dp" />
</android.support.constraint.ConstraintLayout>
